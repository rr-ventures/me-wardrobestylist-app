{
  "name": "notion-wardrobe-stylist",
  "version": "1.0.0-mvp",
  "description": "Personal AI styling system using Notion as data layer and UI, Gemini as styling engine, webhook-triggered automation on Railway",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "type-check": "tsc --noEmit",
    "copy-assets": "mkdir -p dist/public && cp -r src/public/* dist/public/ 2>/dev/null || true",
    "test-setup": "ts-node src/utils/test-setup.ts"
  },
  "keywords": [
    "notion",
    "gemini",
    "wardrobe",
    "styling",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@notionhq/client": "^2.2.15",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
